Algoritmo "Maior_Que_10"
Var
   valor: real
Inicio
   // Solicita ao usuário que insira um valor
   escreva("Digite um valor: ")
   leia(valor)

   // Verifica se o valor é maior que 10
   se (valor > 10) entao
      escreval("É MAIOR QUE 10!")
   senao
      escreval("NÃO É MAIOR QUE 10!")
   fimse

Fimalgoritmo